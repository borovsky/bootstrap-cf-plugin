---
properties:
  uaa:
    jwt:
      signing_key: generate this with 'openssl genrsa -out signing_key.pem 2048'
      verification_key: generate this with 'openssl rsa -pubout -in signing_key.pem -out verification_key.pem'
    cc:
      client_secret: <%= random_string %>
    admin:
      client_secret: <%= random_string %>
    batch:
      username: <%= random_string %>
      password: <%= random_string %>
    clients:
      login:
        secret: <%= random_string %>
      portal:
        secret: <%= random_string %>
      billing:
        secret: <%= random_string %>
      app-direct:
        secret: <%= random_string %>
      support-services:
        secret: <%= random_string %>
      servicesmgmt:
        secret: <%= random_string %>
    scim:
      users:
      - admin|<%= random_string %>|scim.write,scim.read,openid,cloud_controller.admin
      - services|<%= random_string %>|scim.write,scim.read,openid,cloud_controller.admin

  nats:
    user: <%= random_string %>
    password: <%= random_string %>

  cc:
    bulk_api_password: <%= random_string %>
    staging_upload_user: <%= random_string %>
    staging_upload_password: <%= random_string %>
    db_encryption_key: <%= random_string %>

  router:
    status:
      user: <%= random_string %>
      password: <%= random_string %>

  serialization_data_server:
    upload_token: <%= random_string %>

  syslog_aggregator:
    log_email:
      from_email: <%= random_string %>
      from_password: <%= random_string %>
